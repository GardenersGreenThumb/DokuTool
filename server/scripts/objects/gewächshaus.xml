<?xml version="1.0" encoding="UTF-8"?>
<object name="Gewächshaus">
	<section name="Allgemein">
		<input type="number" required="true">
			<label>Quadratmeter</label>
		</input>
	</section>
	<section name="Gießwagen">
		<input type="number" required="true">
			<label>Anzahl</label>
		</input>
		<input type="dropdown" object="gieswagen" required="true">
			<label>Gießwagen</label>
			<help>Angabe der Gießwagennummern in diesem Gewächshaus</help>
		</input>
	</section>
</object>